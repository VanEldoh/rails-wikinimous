<p><%= link_to "add a new article", articles_new_path %></p>


<% @articles.each do |article| %>
 <h1><%= link_to article.title, article_path(article) %></h1>
  <p><%= link_to "delete an article", article_path(article),
      method: :delete,
      data: { confirm: "Are you sure?" } %><p>
<% end %>
